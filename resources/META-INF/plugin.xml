<idea-plugin>
    <id>com.app3null.ApplyMVI</id>
    <name>ApplyMVI</name>
    <version>0.0.1</version>
    <vendor email="Saba@app3null.com" url="https://github.com/Saba-21/ApplyMVI">Â©Saba Khutuashviili</vendor>

    <description>
        <![CDATA[
      An Android studio plugin that helps you apply MVI arch pattern to your project.<br>
      For further information see docs and sample project.<br>
    ]]>
    </description>

    <change-notes>
        <![CDATA[
        Initial Release<br>
    ]]>
    </change-notes>

    <idea-version since-build="145.0"/>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <internalFileTemplate name="Text1"/>
    </extensions>

    <actions>
        <action id="com.app3null.applymvi.action.CreateFileAction"
                class="com.app3null.applymvi.action.CreateFileAction"
                text="ApplyMVI"
                description="CreateFiles">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>

</idea-plugin>